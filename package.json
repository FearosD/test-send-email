{
  "name": "test-send-email",
  "version": "1.0.0",
  "description": "test-send-email-with-js",
  "type": "module",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "predev": "rm -rf dist",
    "dev": "npm run predev && parcel src/index.html --open",
